    "\n",
    "train_time = time.time() - start_time\n",
    "print(f\" –ú–æ–¥–µ–ª—å –æ–±—É—á–µ–Ω–∞ –∑–∞ {train_time:.2f} —Å–µ–∫!\")\n",
    "\n",
    "# ============================================================================\n",
    "# –ü—Ä–µ–¥—Å–∫–∞–∑–∞–Ω–∏—è\n",
    "# ============================================================================\n",
    "\n",
    "print(f\"\\n –î–µ–ª–∞–µ–º –ø—Ä–µ–¥—Å–∫–∞–∑–∞–Ω–∏—è...\")\n",
    "\n",
    "train_pred = np.clip(linear_model.predict(train_X), 0, 1)\n",
    "val_pred = np.clip(linear_model.predict(val_X), 0, 1)\n",
    "\n",
    "# ============================================================================\n",
    "# –û—Ü–µ–Ω–∫–∞ –∫–∞—á–µ—Å—Ç–≤–∞\n",
    "# ============================================================================\n",
    "\n",
    "train_mse = mean_squared_error(train_y, train_pred)\n",
    "val_mse = mean_squared_error(val_y, val_pred)\n",
    "\n",
    "train_mae = mean_absolute_error(train_y, train_pred)\n",
    "val_mae = mean_absolute_error(val_y, val_pred)\n",
    "\n",
    "train_rmse = np.sqrt(train_mse)\n",
    "val_rmse = np.sqrt(val_mse)\n",
    "\n",
    "print(f\"\\n{'=' * 50}\")\n",
    "print(f\"{'–ú–µ—Ç—Ä–∏–∫–∞':<20} {'Train':<15} {'Val':<15}\")\n",
    "print(f\"{'=' * 50}\")\n",
    "print(f\"{'MSE:':<20} {train_mse:<15.6f} {val_mse:<15.6f}\")\n",
    "print(f\"{'MAE:':<20} {train_mae:<15.6f} {val_mae:<15.6f}\")\n",
    "print(f\"{'RMSE:':<20} {train_rmse:<15.6f} {val_rmse:<15.6f}\")\n",
    "print(f\"{'=' * 50}\")\n",
    "\n",
    "# ============================================================================\n",
    "# –î–æ–±–∞–≤–ª—è–µ–º —Ä–µ–∑—É–ª—å—Ç–∞—Ç—ã –≤ —Ç—Ä–µ–∫–µ—Ä\n",
    "# ============================================================================\n",
    "\n",
    "tracker.add_model(\n",
