    "print(\"=\" * 70)\n",
    "print(\" –û–¶–ï–ù–ö–ê –ö–ê–ß–ï–°–¢–í–ê –ü–û–ò–°–ö–ê\")\n",
    "print(\"=\" * 70)\n",
    "\n",
    "# ============================================================================\n",
    "# –¢–µ—Å—Ç–∏—Ä–æ–≤–∞–Ω–∏–µ –Ω–∞ –≤—Å—ë–º –Ω–∞–±–æ—Ä–µ (–±–µ–∑ —é—Ä–∏–¥–∏—á–µ—Å–∫–∏ –∑–∞–ø—Ä–µ—â—ë–Ω–Ω—ã—Ö –∑–∞–ø—Ä–æ—Å–æ–≤)\n",
    "# ============================================================================\n",
    "\n",
    "total_queries = 0\n",
    "correct_top1 = 0\n",
    "correct_top3 = 0\n",
    "correct_top5 = 0\n",
    "prohibited_count = 0\n",
    "\n",
    "print(\"\\n –û—Ü–µ–Ω–∏–≤–∞–µ–º –∫–∞—á–µ—Å—Ç–≤–æ –ø–æ–∏—Å–∫–∞ –Ω–∞ —Ç–µ—Å—Ç–æ–≤–æ–º –Ω–∞–±–æ—Ä–µ...\")\n",
    "print(\"   (–∑–∞–ø—Ä–æ—Å—ã —Å –¥–µ—Ç—å–º–∏ –ø—Ä–æ–ø—É—Å–∫–∞—é—Ç—Å—è —Å–æ–≥–ª–∞—Å–Ω–æ —é—Ä–∏–¥–∏—á–µ—Å–∫–∏–º —Ç—Ä–µ–±–æ–≤–∞–Ω–∏—è–º)\")\n",
    "\n",
    "for idx, row in tqdm(test_queries.iterrows(), total=len(test_queries)):\n",
    "    query_text = str(row['query_text'])\n",
    "    expected_image = row['image']\n",
    "\n",
    "    # –ü—Ä–æ–≤–µ—Ä–∫–∞ –Ω–∞ —é—Ä–∏–¥–∏—á–µ—Å–∫–∏–µ –æ–≥—Ä–∞–Ω–∏—á–µ–Ω–∏—è\n",
    "    query_lower = query_text.lower()\n",
    "    prohibited = any(word in query_lower for word in\n",
    "                     ['child', 'boy', 'girl', 'kid', 'teen', 'youth', 'young'])\n",
    "\n",
    "    if prohibited:\n",
    "        prohibited_count += 1\n",
    "        continue\n",
    "\n",
    "    total_queries += 1\n",
    "\n",
    "    # –ü–æ–∏—Å–∫\n",
    "    try:\n",
    "        tokens = clip.tokenize([query_text], truncate=True).to(device)\n",
    "\n",
    "        with torch.no_grad():\n",
    "            text_embedding = clip_model.encode_text(tokens)\n",
    "            text_embedding = text_embedding / text_embedding.norm(dim=-1, keepdim=True)\n",
    "\n",
    "        text_embedding = text_embedding.squeeze().cpu().numpy()\n",
    "\n",
    "        # –í—ã—á–∏—Å–ª—è–µ–º —Å—Ö–æ–∂–µ—Å—Ç—å —Å–æ –≤—Å–µ–º–∏ –∏–∑–æ–±—Ä–∞–∂–µ–Ω–∏—è–º–∏\n",
    "        similarities = []\n",
    "        for img_name, img_embedding in test_image_embeddings.items():\n",
    "            raw_similarity = np.dot(img_embedding, text_embedding)\n",
    "            calibrated_score = calibrator.predict([[raw_similarity]])[0]\n",
    "            similarities.append((img_name, calibrated_score))\n",
    "\n",
    "        # –°–æ—Ä—Ç–∏—Ä—É–µ–º\n",
    "        similarities.sort(key=lambda x: x[1], reverse=True)\n",
    "\n",
    "        # –û—Ü–µ–Ω–∏–≤–∞–µ–º —Ç–æ—á–Ω–æ—Å—Ç—å\n",
    "        top1 = similarities[0][0]\n",
    "        top3 = [x[0] for x in similarities[:3]]\n",
    "        top5 = [x[0] for x in similarities[:5]]\n",
    "\n",
    "        if top1 == expected_image:\n",
    "            correct_top1 += 1\n",
    "        if expected_image in top3:\n",
    "            correct_top3 += 1\n",
    "        if expected_image in top5:\n",
    "            correct_top5 += 1\n",
    "\n",
    "    except Exception as e:\n",
    "        continue\n",
    "\n",
    "# ============================================================================\n",
    "# –í—ã–≤–æ–¥ —Ä–µ–∑—É–ª—å—Ç–∞—Ç–æ–≤\n",
    "# ============================================================================\n",
    "\n",
    "print(f\"\\n{'=' * 70}\")\n",
    "print(f\"–†–ï–ó–£–õ–¨–¢–ê–¢–´ –û–¶–ï–ù–ö–ò –ö–ê–ß–ï–°–¢–í–ê\")\n",
    "print(f\"{'=' * 70}\")\n",
    "\n",
    "print(f\"\\n–û–±—Ä–∞–±–æ—Ç–∞–Ω–æ –∑–∞–ø—Ä–æ—Å–æ–≤: {total_queries} –∏–∑ {len(test_queries)}\")\n",
    "print(f\"–ü—Ä–æ–ø—É—â–µ–Ω–æ (—é—Ä–∏–¥–∏—á–µ—Å–∫–∏–µ –æ–≥—Ä–∞–Ω–∏—á–µ–Ω–∏—è): {prohibited_count}\")\n",
    "\n",
    "print(f\"\\n–¢–æ—á–Ω–æ—Å—Ç—å –ø–æ–∏—Å–∫–∞:\")\n",
    "print(f\"   Top-1 accuracy:  {correct_top1 / total_queries * 100:.1f}% ({correct_top1}/{total_queries})\")\n",
    "print(f\"   Top-3 accuracy:  {correct_top3 / total_queries * 100:.1f}% ({correct_top3}/{total_queries})\")\n",
    "print(f\"   Top-5 accuracy:  {correct_top5 / total_queries * 100:.1f}% ({correct_top5}/{total_queries})\")\n",
    "\n",
    "# –í–∏–∑—É–∞–ª–∏–∑–∞—Ü–∏—è\n",
    "fig, ax = plt.subplots(figsize=(8, 5))\n",
    "metrics = ['Top-1', 'Top-3', 'Top-5']\n",
    "values = [\n",
    "    correct_top1 / total_queries * 100,\n",
    "    correct_top3 / total_queries * 100,\n",
    "    correct_top5 / total_queries * 100\n",
    "]\n",
    "colors = ['coral', 'skyblue', 'lightgreen']\n",
    "\n",
    "bars = ax.bar(metrics, values, color=colors)\n",
    "ax.set_ylabel('–¢–æ—á–Ω–æ—Å—Ç—å (%)')\n",
    "ax.set_title('–ö–∞—á–µ—Å—Ç–≤–æ –ø–æ–∏—Å–∫–∞ –∏–∑–æ–±—Ä–∞–∂–µ–Ω–∏–π –ø–æ —Ç–µ–∫—Å—Ç–æ–≤–æ–º—É –∑–∞–ø—Ä–æ—Å—É')\n",
    "ax.set_ylim(0, 100)\n",
    "\n",
    "for bar, value in zip(bars, values):\n",
    "    height = bar.get_height()\n",
    "    ax.text(bar.get_x() + bar.get_width()/2., height,\n",
    "            f'{value:.1f}%', ha='center', va='bottom', fontsize=12)\n",
    "\n",
    "plt.grid(axis='y', alpha=0.3)\n",
    "plt.tight_layout()\n",
    "plt.show()\n",
    "\n",
    "print(f\"\\n –û—Ü–µ–Ω–∫–∞ –∫–∞—á–µ—Å—Ç–≤–∞ –∑–∞–≤–µ—Ä—à–µ–Ω–∞!\")"
   ]
