    "    fig, axes = plt.subplots(2, 4, figsize=(20, 10))\n",
    "    axes = axes.flatten()\n",
    "\n",
    "    for i, img_name in enumerate(files[:8]):\n",
    "        img_path = os.path.join(folder_path, img_name)\n",
    "        try:\n",
