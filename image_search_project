   ],
   "source": [
    "# –ê–Ω–∞–ª–∏–∑ –∏–∑–æ–±—Ä–∞–∂–µ–Ω–∏–π\n",
    "print(\"=\" * 70)\n",
    "print(\"–ê–ù–ê–õ–ò–ó –ò–ó–û–ë–†–ê–ñ–ï–ù–ò–ô\")\n",
    "print(\"=\" * 70)\n",
    "\n",
    "# –ü—É—Ç–∏ –∫ –∏–∑–æ–±—Ä–∞–∂–µ–Ω–∏—è–º\n",
    "TRAIN_IMAGES_PATH = '/content/project_data/to_upload/train_images'\n",
    "TEST_IMAGES_PATH = '/content/project_data/to_upload/test_images'\n",
    "\n",
    "def analyze_image_folder(folder_path, folder_name):\n",
    "    \"\"\"–ê–Ω–∞–ª–∏–∑–∏—Ä—É–µ—Ç –ø–∞–ø–∫—É —Å –∏–∑–æ–±—Ä–∞–∂–µ–Ω–∏—è–º–∏\"\"\"\n",
    "    print(f\"\\n{'=' * 70}\")\n",
    "    print(f\"Ô∏è {folder_name}\")\n",
    "    print(f\"{'=' * 70}\")\n",
    "\n",
    "    valid_extensions = ('.jpg', '.jpeg', '.png', '.JPG', '.JPEG', '.PNG')\n",
    "    files = [f for f in os.listdir(folder_path) if f.endswith(valid_extensions)]\n",
    "\n",
    "    print(f\"\\n –ö–æ–ª–∏—á–µ—Å—Ç–≤–æ –∏–∑–æ–±—Ä–∞–∂–µ–Ω–∏–π: {len(files)}\")\n",
    "\n",
    "    # –ê–Ω–∞–ª–∏–∑–∏—Ä—É–µ–º —Ä–∞–∑–º–µ—Ä—ã, —Ñ–æ—Ä–º–∞—Ç—ã –∏ –±–∏—Ç—ã–µ —Ñ–∞–π–ª—ã\n",
    "    widths = []\n",
    "    heights = []\n",
    "    formats = {}\n",
    "    broken_images = []\n",
    "    file_sizes = []\n",
    "\n",
    "    for img_name in files:\n",
    "        img_path = os.path.join(folder_path, img_name)\n",
    "        try:\n",
    "            with Image.open(img_path) as img:\n",
    "                widths.append(img.width)\n",
    "                heights.append(img.height)\n",
    "                fmt = img.format\n",
    "                formats[fmt] = formats.get(fmt, 0) + 1\n",
    "                file_sizes.append(os.path.getsize(img_path) / 1024)  # KB\n",
    "        except Exception as e:\n",
    "            broken_images.append((img_name, str(e)))\n",
    "\n",
    "    # –°—Ç–∞—Ç–∏—Å—Ç–∏–∫–∞\n",
    "    print(f\"\\n –§–æ—Ä–º–∞—Ç—ã —Ñ–∞–π–ª–æ–≤: {formats}\")\n",
    "    print(f\" –ë–∏—Ç—ã—Ö –∏–∑–æ–±—Ä–∞–∂–µ–Ω–∏–π: {len(broken_images)}\")\n",
    "    if broken_images:\n",
    "        print(f\"   –°–ø–∏—Å–æ–∫: {broken_images[:10]}\")\n",
    "\n",
    "    print(f\"\\n –†–∞–∑–º–µ—Ä—ã –∏–∑–æ–±—Ä–∞–∂–µ–Ω–∏–π:\")\n",
    "    print(f\"   –®–∏—Ä–∏–Ω–∞: –º–∏–Ω={min(widths)}, –º–∞–∫—Å={max(widths)}, –º–µ–¥–∏–∞–Ω–∞={np.median(widths):.0f}\")\n",
    "    print(f\"   –í—ã—Å–æ—Ç–∞: –º–∏–Ω={min(heights)}, –º–∞–∫—Å={max(heights)}, –º–µ–¥–∏–∞–Ω–∞={np.median(heights):.0f}\")\n",
    "\n",
    "    print(f\"\\n –†–∞–∑–º–µ—Ä —Ñ–∞–π–ª–æ–≤:\")\n",
    "    print(f\"   –ú–∏–Ω: {min(file_sizes):.1f} KB\")\n",
    "    print(f\"   –ú–∞–∫—Å: {max(file_sizes):.1f} KB\")\n",
    "    print(f\"   –°—Ä–µ–¥–Ω–∏–π: {np.mean(file_sizes):.1f} KB\")\n",
    "\n",
    "    # –í–∏–∑—É–∞–ª–∏–∑–∞—Ü–∏—è\n",
    "    fig, axes = plt.subplots(2, 2, figsize=(14, 10))\n",
    "\n",
    "    # 1. –†–∞—Å–ø—Ä–µ–¥–µ–ª–µ–Ω–∏–µ —à–∏—Ä–∏–Ω—ã\n",
    "    sns.histplot(widths, ax=axes[0, 0], kde=True, color='skyblue')\n",
    "    axes[0, 0].set_title(f'{folder_name}: –†–∞—Å–ø—Ä–µ–¥–µ–ª–µ–Ω–∏–µ —à–∏—Ä–∏–Ω—ã')\n",
    "    axes[0, 0].set_xlabel('–ü–∏–∫—Å–µ–ª–∏')\n",
    "    axes[0, 0].set_ylabel('–ö–æ–ª–∏—á–µ—Å—Ç–≤–æ')\n",
    "\n",
    "    # 2. –†–∞—Å–ø—Ä–µ–¥–µ–ª–µ–Ω–∏–µ –≤—ã—Å–æ—Ç—ã\n",
    "    sns.histplot(heights, ax=axes[0, 1], kde=True, color='coral')\n",
    "    axes[0, 1].set_title(f'{folder_name}: –†–∞—Å–ø—Ä–µ–¥–µ–ª–µ–Ω–∏–µ –≤—ã—Å–æ—Ç—ã')\n",
    "    axes[0, 1].set_xlabel('–ü–∏–∫—Å–µ–ª–∏')\n",
    "    axes[0, 1].set_ylabel('–ö–æ–ª–∏—á–µ—Å—Ç–≤–æ')\n",
    "\n",
    "    # 3. –°–æ–æ—Ç–Ω–æ—à–µ–Ω–∏–µ —Å—Ç–æ—Ä–æ–Ω\n",
    "    aspect_ratios = [w/h for w, h in zip(widths, heights)]\n",
    "    sns.histplot(aspect_ratios, ax=axes[1, 0], kde=True, color='green')\n",
    "    axes[1, 0].set_title(f'{folder_name}: –°–æ–æ—Ç–Ω–æ—à–µ–Ω–∏–µ —Å—Ç–æ—Ä–æ–Ω (—à–∏—Ä–∏–Ω–∞/–≤—ã—Å–æ—Ç–∞)')\n",
    "    axes[1, 0].set_xlabel('Aspect Ratio')\n",
    "    axes[1, 0].set_ylabel('–ö–æ–ª–∏—á–µ—Å—Ç–≤–æ')\n",
