    "#–ó–∞–≥—Ä—É–∑–∫–∞ —Ç–∞–±–ª–∏—Ü –≤ DataFrame\n",
    "\n",
    "# 1. train_dataset.csv\n",
    "train_dataset = pd.read_csv(os.path.join(DATA_PATH, 'train_dataset.csv'))\n",
    "print(f\"‚úÖ train_dataset: {train_dataset.shape}\")\n",
    "\n",
    "# 2. ExpertAnnotations.tsv (–±–µ–∑ –∑–∞–≥–æ–ª–æ–≤–∫–∞)\n",
    "expert_annotations = pd.read_csv(\n",
    "    os.path.join(DATA_PATH, 'ExpertAnnotations.tsv'),\n",
    "    sep='\\t',\n",
    "    header=None,\n",
    "    names=['image', 'query_id', 'expert_1', 'expert_2', 'expert_3'],\n",
    "    on_bad_lines='skip'\n",
    ")\n",
    "print(f\"‚úÖ ExpertAnnotations: {expert_annotations.shape}\")\n",
    "\n",
    "# 3. CrowdAnnotations.tsv (–±–µ–∑ –∑–∞–≥–æ–ª–æ–≤–∫–∞)\n",
    "crowd_annotations = pd.read_csv(\n",
    "    os.path.join(DATA_PATH, 'CrowdAnnotations.tsv'),\n",
    "    sep='\\t',\n",
    "    header=None,\n",
    "    names=['image', 'query_id', 'share_confirm', 'count_confirm', 'count_reject'],\n",
    "    on_bad_lines='skip'\n",
    ")\n",
    "print(f\"‚úÖ CrowdAnnotations: {crowd_annotations.shape}\")\n",
    "\n",
    "# 4. test_queries.csv (—Ä–∞–∑–¥–µ–ª–∏—Ç–µ–ª—å |)\n",
    "test_queries = pd.read_csv(\n",
    "    os.path.join(DATA_PATH, 'test_queries.csv'),\n",
    "    sep='|',\n",
    "    on_bad_lines='skip'\n",
    ")\n",
    "test_queries.columns = test_queries.columns.str.strip()\n",
    "# –£–¥–∞–ª—è–µ–º –ø—É—Å—Ç—É—é –∫–æ–ª–æ–Ω–∫—É Unnamed: 0 –µ—Å–ª–∏ –µ—Å—Ç—å\n",
    "if 'Unnamed: 0' in test_queries.columns:\n",
    "    test_queries = test_queries.drop(columns=['Unnamed: 0'])\n",
    "print(f\"‚úÖ test_queries: {test_queries.shape}\")\n",
    "\n",
    "# 5. test_images.csv\n",
    "try:\n",
    "    test_images_df = pd.read_csv(os.path.join(DATA_PATH, 'test_images.csv'))\n",
    "    print(f\"‚úÖ test_images.csv: {test_images_df.shape}\")\n",
    "except:\n",
    "    test_images_df = None\n",
