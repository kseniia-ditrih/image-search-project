   ],
   "source": [
    "# –†–∞–∑–±–∏–µ–Ω–∏–µ –¥–∞–Ω–Ω—ã—Ö –Ω–∞ train/val\n",
    "\n",
    "print(\"=\" * 70)\n",
    "print(\" –†–ê–ó–ë–ò–ï–ù–ò–ï –ù–ê TRAIN/VAL\")\n",
    "print(\"=\" * 70)\n",
    "\n",
    "# –ü–æ–¥–≥–æ—Ç–æ–≤–∫–∞ –¥–∞–Ω–Ω—ã—Ö –¥–ª—è —Å–ø–ª–∏—Ç–∞\n",
    "df_split = train_df_final.copy()\n",
    "\n",
    "# –°–±—Ä–∞—Å—ã–≤–∞–µ–º –∏–Ω–¥–µ–∫—Å—ã, —á—Ç–æ–±—ã –æ–Ω–∏ –±—ã–ª–∏ –ø–æ—Å–ª–µ–¥–æ–≤–∞—Ç–µ–ª—å–Ω—ã–º–∏ (0, 1, 2, 3...)\n",
    "df_split = df_split.reset_index(drop=True)\n",
    "\n",
    "# X ‚Äî –ø—Ä–∏–∑–Ω–∞–∫–∏ (–ø–æ–∫–∞ —Ç–µ–∫—Å—Ç—ã, –∏–∑–æ–±—Ä–∞–∂–µ–Ω–∏—è –¥–æ–±–∞–≤–∏–º –ø–æ–∑–∂–µ)\n",
    "X = df_split[['query_text', 'image']]\n",
    "# y ‚Äî —Ü–µ–ª–µ–≤–∞—è –ø–µ—Ä–µ–º–µ–Ω–Ω–∞—è\n",
    "y = df_split['target_norm']\n",
    "# groups ‚Äî –ø–æ —á–µ–º—É –≥—Ä—É–ø–ø–∏—Ä—É–µ–º (—á—Ç–æ–±—ã –æ–¥–Ω–æ –∏–∑–æ–±—Ä–∞–∂–µ–Ω–∏–µ –±—ã–ª–æ —Ç–æ–ª—å–∫–æ –≤ –æ–¥–Ω–æ–π –≤—ã–±–æ—Ä–∫–µ)\n",
    "groups = df_split['image']\n",
    "\n",
    "# –°–æ–∑–¥–∞—ë–º —Å–ø–ª–∏—Ç—Ç–µ—Ä\n",
    "gss = GroupShuffleSplit(n_splits=1, train_size=0.7, test_size=0.3, random_state=42)\n",
    "\n",
    "# –ü–æ–ª—É—á–∞–µ–º –∏–Ω–¥–µ–∫—Å—ã\n",
    "train_indices, val_indices = next(gss.split(X, y, groups))\n",
    "\n",
    "# –†–∞–∑–±–∏–≤–∞–µ–º –¥–∞—Ç–∞—Å–µ—Ç\n",
    "train_df = df_split.loc[train_indices].reset_index(drop=True)\n",
    "val_df = df_split.loc[val_indices].reset_index(drop=True)\n",
    "\n",
    "print(f\"\\n –†–µ–∑—É–ª—å—Ç–∞—Ç—ã —Ä–∞–∑–±–∏–µ–Ω–∏—è:\")\n",
    "print(f\"   –í—Å–µ–≥–æ –∑–∞–ø–∏—Å–µ–π: {len(df_split)}\")\n",
    "print(f\"   Train: {len(train_df)} ({len(train_df)/len(df_split)*100:.1f}%)\")\n",
    "print(f\"   Val: {len(val_df)} ({len(val_df)/len(df_split)*100:.1f}%)\")\n",
    "\n",
    "# –ü—Ä–æ–≤–µ—Ä—è–µ–º, —á—Ç–æ –∏–∑–æ–±—Ä–∞–∂–µ–Ω–∏—è –Ω–µ –ø–µ—Ä–µ—Å–µ–∫–∞—é—Ç—Å—è\n",
    "train_images = set(train_df['image'].unique())\n",
    "val_images = set(val_df['image'].unique())\n",
    "overlap = train_images & val_images\n",
    "\n",
    "print(f\"\\n –ü—Ä–æ–≤–µ—Ä–∫–∞ –ø–µ—Ä–µ—Å–µ—á–µ–Ω–∏–π:\")\n",
    "print(f\"   –£–Ω–∏–∫–∞–ª—å–Ω—ã—Ö –∏–∑–æ–±—Ä–∞–∂–µ–Ω–∏–π –≤ train: {len(train_images)}\")\n",
    "print(f\"   –£–Ω–∏–∫–∞–ª—å–Ω—ã—Ö –∏–∑–æ–±—Ä–∞–∂–µ–Ω–∏–π –≤ val: {len(val_images)}\")\n",
    "print(f\"   –ü–µ—Ä–µ—Å–µ–∫–∞—é—â–∏—Ö—Å—è –∏–∑–æ–±—Ä–∞–∂–µ–Ω–∏–π: {len(overlap)}\")\n",
    "\n",
    "if len(overlap) == 0:\n",
    "    print(f\" –ü—Ä–æ–≤–µ—Ä–∫–∞ –ø—Ä–æ–π–¥–µ–Ω–∞: –∏–∑–æ–±—Ä–∞–∂–µ–Ω–∏—è –Ω–µ –ø–µ—Ä–µ—Å–µ–∫–∞—é—Ç—Å—è!\")\n",
    "else:\n",
