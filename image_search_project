    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "\n",
      " MLP –æ–±—É—á–µ–Ω–∞ –∏ –¥–æ–±–∞–≤–ª–µ–Ω–∞ –≤ tracker!\n"
     ]
    }
   ],
   "source": [
    "print(\"=\" * 70)\n",
    "print(\"–û–ë–£–ß–ï–ù–ò–ï –ü–û–õ–ù–û–°–í–Ø–ó–ù–û–ô –ù–ï–ô–†–û–°–ï–¢–ò (MLP)\")\n",
    "print(\"=\" * 70)\n",
    "\n",
    "# ============================================================================\n",
    "# –ü—Ä–æ–±–ª–µ–º–∞ 1: –î–∏—Å–±–∞–ª–∞–Ω—Å –∫–ª–∞—Å—Å–æ–≤\n",
    "# –†–µ—à–µ–Ω–∏–µ: –í–∑–≤–µ—à–µ–Ω–Ω–∞—è —Ñ—É–Ω–∫—Ü–∏—è –ø–æ—Ç–µ—Ä—å\n",
    "# ============================================================================\n",
    "\n",
    "print(f\"\\n –£—á—ë—Ç –¥–∏—Å–±–∞–ª–∞–Ω—Å–∞ –∫–ª–∞—Å—Å–æ–≤...\")\n",
    "\n",
    "# –°—á–∏—Ç–∞–µ–º –≤–µ—Å–∞ –¥–ª—è –∫–∞–∂–¥–æ–≥–æ –∑–Ω–∞—á–µ–Ω–∏—è target\n",
    "# –ß–µ–º —Ä–µ–∂–µ –∫–ª–∞—Å—Å, —Ç–µ–º –≤—ã—à–µ –≤–µ—Å\n",
    "unique_targets, counts = np.unique(train_y, return_counts=True)\n",
    "class_weights = 1.0 / counts\n",
    "class_weights = class_weights / class_weights.sum() * len(unique_targets)\n",
    "\n",
    "# –°–æ–∑–¥–∞—ë–º —Å–ª–æ–≤–∞—Ä—å –≤–µ—Å–æ–≤ –¥–ª—è –∫–∞–∂–¥–æ–π –∑–∞–ø–∏—Å–∏\n",
    "sample_weights = np.array([class_weights[list(unique_targets).index(y)] for y in train_y])\n",
    "sample_weights = torch.FloatTensor(sample_weights)\n",
    "\n",
    "print(f\"   –£–Ω–∏–∫–∞–ª—å–Ω—ã—Ö –∑–Ω–∞—á–µ–Ω–∏–π target: {len(unique_targets)}\")\n",
    "print(f\"   –†–∞—Å–ø—Ä–µ–¥–µ–ª–µ–Ω–∏–µ: {dict(zip(unique_targets, counts))}\")\n",
    "print(f\"   –í–µ—Å–∞ –∫–ª–∞—Å—Å–æ–≤: {class_weights}\")\n",
    "\n",
    "# ============================================================================\n",
    "# –ü—Ä–æ–±–ª–µ–º–∞ 2: –ü–µ—Ä–µ–æ–±—É—á–µ–Ω–∏–µ\n",
    "# –†–µ—à–µ–Ω–∏–µ: Dropout + BatchNorm + Early Stopping + L2 —Ä–µ–≥—É–ª—è—Ä–∏–∑–∞—Ü–∏—è\n",
    "# ============================================================================\n",
    "\n",
    "class MLPModel(nn.Module):\n",
    "    \"\"\"–ü–æ–ª–Ω–æ—Å–≤—è–∑–Ω–∞—è –Ω–µ–π—Ä–æ—Å–µ—Ç—å —Å Dropout –∏ BatchNorm\"\"\"\n",
    "\n",
    "    def __init__(self, input_dim, hidden_layers=[512, 256, 128], dropout=0.3):\n",
    "        super(MLPModel, self).__init__()\n",
    "\n",
    "        layers = []\n",
    "        prev_dim = input_dim\n",
    "\n",
    "        for hidden_dim in hidden_layers:\n",
    "            layers.append(nn.Linear(prev_dim, hidden_dim))\n",
    "            layers.append(nn.BatchNorm1d(hidden_dim))\n",
    "            layers.append(nn.ReLU())\n",
    "            layers.append(nn.Dropout(dropout))\n",
    "            prev_dim = hidden_dim\n",
    "\n",
    "        # –í—ã—Ö–æ–¥–Ω–æ–π —Å–ª–æ–π (1 –Ω–µ–π—Ä–æ–Ω –¥–ª—è —Ä–µ–≥—Ä–µ—Å—Å–∏–∏)\n",
    "        layers.append(nn.Linear(prev_dim, 1))\n",
    "        layers.append(nn.Sigmoid())  # –û–≥—Ä–∞–Ω–∏—á–∏–≤–∞–µ–º –≤—ã—Ö–æ–¥ [0, 1]\n",
    "\n",
    "        self.network = nn.Sequential(*layers)\n",
    "\n",
    "    def forward(self, x):\n",
    "        return self.network(x).squeeze()\n",
    "\n",
    "# ============================================================================\n",
    "# –§—É–Ω–∫—Ü–∏—è –¥–ª—è –æ–±—É—á–µ–Ω–∏—è –æ–¥–Ω–æ–π –º–æ–¥–µ–ª–∏\n",
    "# ============================================================================\n",
    "\n",
    "def train_mlp(train_X, train_y, val_X, val_y,\n",
    "              hidden_layers=[512, 256, 128],\n",
    "              dropout=0.3,\n",
    "              learning_rate=0.001,\n",
    "              epochs=100,\n",
    "              batch_size=32,\n",
    "              use_weights=True,\n",
    "              patience=10):\n",
    "    \"\"\"–û–±—É—á–∞–µ—Ç MLP —Å early stopping\"\"\"\n",
    "\n",
    "    device = torch.device('cuda' if torch.cuda.is_available() else 'cpu')\n",
    "\n",
    "    # –°–æ–∑–¥–∞—ë–º –º–æ–¥–µ–ª—å\n",
    "    model = MLPModel(\n",
    "        input_dim=train_X.shape[1],\n",
    "        hidden_layers=hidden_layers,\n",
    "        dropout=dropout\n",
    "    ).to(device)\n",
    "\n",
    "    # –°–æ–∑–¥–∞—ë–º DataLoader\n",
    "    train_dataset = TensorDataset(\n",
    "        torch.FloatTensor(train_X),\n",
    "        torch.FloatTensor(train_y),\n",
    "        sample_weights if use_weights else torch.ones(len(train_y))\n",
    "    )\n",
    "    train_loader = DataLoader(train_dataset, batch_size=batch_size, shuffle=True)\n",
    "\n",
    "    # –§—É–Ω–∫—Ü–∏—è –ø–æ—Ç–µ—Ä—å —Å –≤–µ—Å–∞–º–∏\n",
    "    criterion = nn.MSELoss(reduction='none')\n",
    "    optimizer = optim.Adam(model.parameters(), lr=learning_rate, weight_decay=1e-5)\n",
    "    scheduler = optim.lr_scheduler.ReduceLROnPlateau(optimizer, mode='min', factor=0.5, patience=5)\n",
    "\n",
    "    # Early stopping\n",
    "    best_val_loss = float('inf')\n",
    "    patience_counter = 0\n",
    "    best_model_state = None\n",
    "\n",
    "    train_losses = []\n",
    "    val_losses = []\n",
    "\n",
    "    start_time = time.time()\n",
    "\n",
    "    for epoch in range(epochs):\n",
    "        # Training\n",
    "        model.train()\n",
    "        epoch_train_loss = 0\n",
    "\n",
    "        for X_batch, y_batch, weights_batch in train_loader:\n",
    "            X_batch, y_batch, weights_batch = X_batch.to(device), y_batch.to(device), weights_batch.to(device)\n",
    "\n",
    "            optimizer.zero_grad()\n",
    "            predictions = model(X_batch)\n",
    "\n",
    "            # –í–∑–≤–µ—à–µ–Ω–Ω—ã–π loss\n",
    "            loss = criterion(predictions, y_batch)\n",
    "            weighted_loss = (loss * weights_batch).mean()\n",
    "\n",
    "            weighted_loss.backward()\n",
    "            optimizer.step()\n",
    "\n",
    "            epoch_train_loss += weighted_loss.item()\n",
    "\n",
    "        avg_train_loss = epoch_train_loss / len(train_loader)\n",
    "        train_losses.append(avg_train_loss)\n",
    "\n",
    "        # Validation\n",
    "        model.eval()\n",
    "        with torch.no_grad():\n",
    "            val_pred = model(torch.FloatTensor(val_X).to(device)).cpu().numpy()\n",
    "            val_loss = mean_squared_error(val_y, val_pred)\n",
    "            val_losses.append(val_loss)\n",
    "\n",
    "        scheduler.step(val_loss)\n",
    "\n",
    "        # Early stopping check\n",
    "        if val_loss < best_val_loss:\n",
    "            best_val_loss = val_loss\n",
    "            patience_counter = 0\n",
    "            best_model_state = model.state_dict().copy()\n",
    "        else:\n",
    "            patience_counter += 1\n",
    "            if patience_counter >= patience:\n",
    "                print(f\"   Early stopping –Ω–∞ —ç–ø–æ—Ö–µ {epoch+1}\")\n",
    "                break\n",
    "\n",
    "        # –í—ã–≤–æ–¥ –ø—Ä–æ–≥—Ä–µ—Å—Å–∞ –∫–∞–∂–¥—ã–µ 10 —ç–ø–æ—Ö\n",
    "        if (epoch + 1) % 10 == 0:\n",
    "            print(f\"   Epoch {epoch+1}/{epochs}: Train Loss={avg_train_loss:.6f}, Val MSE={val_loss:.6f}\")\n",
    "\n",
    "    train_time = time.time() - start_time\n",
    "\n",
    "    # –ó–∞–≥—Ä—É–∂–∞–µ–º –ª—É—á—à—É—é –º–æ–¥–µ–ª—å\n",
    "    if best_model_state is not None:\n",
    "        model.load_state_dict(best_model_state)\n",
    "\n",
    "    # –§–∏–Ω–∞–ª—å–Ω—ã–µ –ø—Ä–µ–¥—Å–∫–∞–∑–∞–Ω–∏—è\n",
    "    model.eval()\n",
    "    with torch.no_grad():\n",
    "        train_pred = model(torch.FloatTensor(train_X).to(device)).cpu().numpy()\n",
    "        val_pred = model(torch.FloatTensor(val_X).to(device)).cpu().numpy()\n",
    "\n",
    "    # –û–≥—Ä–∞–Ω–∏—á–∏–≤–∞–µ–º [0, 1]\n",
    "    train_pred = np.clip(train_pred, 0, 1)\n",
    "    val_pred = np.clip(val_pred, 0, 1)\n",
    "\n",
    "    # –ú–µ—Ç—Ä–∏–∫–∏\n",
    "    train_mse = mean_squared_error(train_y, train_pred)\n",
    "    val_mse = mean_squared_error(val_y, val_pred)\n",
    "    train_mae = mean_absolute_error(train_y, train_pred)\n",
    "    val_mae = mean_absolute_error(val_y, val_pred)\n",
    "\n",
    "    results = {\n",
    "        'model': model,\n",
    "        'train_mse': train_mse,\n",
    "        'val_mse': val_mse,\n",
    "        'train_mae': train_mae,\n",
    "        'val_mae': val_mae,\n",
    "        'train_rmse': np.sqrt(train_mse),\n",
    "        'val_rmse': np.sqrt(val_mse),\n",
    "        'train_time': train_time,\n",
    "        'train_losses': train_losses,\n",
    "        'val_losses': val_losses,\n",
    "        'val_pred': val_pred\n",
    "    }\n",
    "\n",
    "    return results\n",
    "\n",
    "# ============================================================================\n",
    "# –ü–æ–¥–±–æ—Ä –≥–∏–ø–µ—Ä–ø–∞—Ä–∞–º–µ—Ç—Ä–æ–≤ ‚Äî —Ç–µ—Å—Ç–∏—Ä—É–µ–º –Ω–µ—Å–∫–æ–ª—å–∫–æ –∫–æ–Ω—Ñ–∏–≥—É—Ä–∞—Ü–∏–π\n",
    "# ============================================================================\n",
    "\n",
    "print(f\"\\n –ü–æ–¥–±–æ—Ä –≥–∏–ø–µ—Ä–ø–∞—Ä–∞–º–µ—Ç—Ä–æ–≤...\")\n",
    "print(f\"   –¢–µ—Å—Ç–∏—Ä—É–µ–º {5} –∫–æ–Ω—Ñ–∏–≥—É—Ä–∞—Ü–∏–π MLP\")\n",
    "\n",
    "configs = [\n",
    "    {\n",
    "        'name': 'MLP (v1 - –ª—ë–≥–∫–∞—è)',\n",
    "        'hidden_layers': [256, 128],\n",
    "        'dropout': 0.3,\n",
    "        'learning_rate': 0.001,\n",
    "        'epochs': 100\n",
    "    },\n",
    "    {\n",
    "        'name': 'MLP (v2 - —Å—Ä–µ–¥–Ω—è—è)',\n",
    "        'hidden_layers': [512, 256, 128],\n",
    "        'dropout': 0.3,\n",
    "        'learning_rate': 0.001,\n",
    "        'epochs': 100\n",
    "    },\n",
    "    {\n",
    "        'name': 'MLP (v3 - –≥–ª—É–±–æ–∫–∞—è)',\n",
    "        'hidden_layers': [512, 256, 128, 64],\n",
    "        'dropout': 0.3,\n",
    "        'learning_rate': 0.001,\n",
    "        'epochs': 100\n",
    "    },\n",
    "    {\n",
    "        'name': 'MLP (v4 - –Ω–∏–∑–∫–∏–π dropout)',\n",
    "        'hidden_layers': [512, 256, 128],\n",
    "        'dropout': 0.2,\n",
    "        'learning_rate': 0.001,\n",
    "        'epochs': 100\n",
    "    },\n",
    "    {\n",
    "        'name': 'MLP (v5 - –≤—ã—Å–æ–∫–∏–π dropout)',\n",
    "        'hidden_layers': [512, 256, 128],\n",
    "        'dropout': 0.5,\n",
    "        'learning_rate': 0.001,\n",
    "        'epochs': 100\n",
    "    }\n",
    "]\n",
    "\n",
    "mlp_results = []\n",
    "\n",
    "for i, config in enumerate(configs, 1):\n",
    "    print(f\"\\n{'=' * 70}\")\n",
    "    print(f\" –ö–æ–Ω—Ñ–∏–≥—É—Ä–∞—Ü–∏—è {i}/{len(configs)}: {config['name']}\")\n",
    "    print(f\"{'=' * 70}\")\n",
    "    print(f\"   –°–ª–æ–∏: {config['hidden_layers']}\")\n",
    "    print(f\"   Dropout: {config['dropout']}\")\n",
    "    print(f\"   Learning rate: {config['learning_rate']}\")\n",
    "    print(f\"   Epochs: {config['epochs']}\")\n",
    "    print()\n",
    "\n",
    "    results = train_mlp(\n",
    "        train_X, train_y, val_X, val_y,\n",
    "        hidden_layers=config['hidden_layers'],\n",
    "        dropout=config['dropout'],\n",
    "        learning_rate=config['learning_rate'],\n",
    "        epochs=config['epochs'],\n",
    "        batch_size=32,\n",
    "        use_weights=True,\n",
    "        patience=15\n",
    "    )\n",
    "\n",
    "    results['name'] = config['name']\n",
    "    results['hyperparams'] = f\"layers={config['hidden_layers']}, dropout={config['dropout']}, lr={config['learning_rate']}\"\n",
    "    mlp_results.append(results)\n",
    "\n",
    "    print(f\"\\n  {config['name']} –æ–±—É—á–µ–Ω–∞!\")\n",
    "    print(f\" Val MSE: {results['val_mse']:.6f}\")\n",
    "    print(f\" Val MAE: {results['val_mae']:.6f}\")\n",
    "    print(f\" –í—Ä–µ–º—è: {results['train_time']:.2f} —Å–µ–∫\")\n",
    "\n",
    "# ============================================================================\n",
    "# –í—ã–±–∏—Ä–∞–µ–º –ª—É—á—à—É—é –º–æ–¥–µ–ª—å MLP\n",
    "# ============================================================================\n",
    "\n",
    "print(f\"\\n{'=' * 70}\")\n",
    "print(f\"–í–´–ë–û–† –õ–£–ß–®–ï–ô –ú–û–î–ï–õ–ò MLP\")\n",
    "print(f\"{'=' * 70}\")\n",
    "\n",
    "best_mlp = min(mlp_results, key=lambda x: x['val_mse'])\n",
    "\n",
    "print(f\"\\n   –õ—É—á—à–∞—è –º–æ–¥–µ–ª—å: {best_mlp['name']}\")\n",
    "print(f\"   Val MSE: {best_mlp['val_mse']:.6f}\")\n",
    "print(f\"   Val MAE: {best_mlp['val_mae']:.6f}\")\n",
    "print(f\"   –ì–∏–ø–µ—Ä–ø–∞—Ä–∞–º–µ—Ç—Ä—ã: {best_mlp['hyperparams']}\")\n",
    "\n",
    "# –î–æ–±–∞–≤–ª—è–µ–º –ª—É—á—à—É—é MLP –≤ —Ç—Ä–µ–∫–µ—Ä\n",
    "tracker.add_model(\n",
    "    model_name=f\"MLP (Best) - {best_mlp['name']}\",\n",
    "    model_obj=best_mlp['model'],\n",
    "    train_metrics={'mse': best_mlp['train_mse'], 'mae': best_mlp['train_mae'], 'rmse': best_mlp['train_rmse']},\n",
    "    val_metrics={'mse': best_mlp['val_mse'], 'mae': best_mlp['val_mae'], 'rmse': best_mlp['val_rmse']},\n",
    "    hyperparams=best_mlp['hyperparams'],\n",
    "    train_time=f\"{best_mlp['train_time']:.2f} —Å–µ–∫\",\n",
    "    notes=\"–õ—É—á—à–∞—è –∏–∑ 5 –∫–æ–Ω—Ñ–∏–≥—É—Ä–∞—Ü–∏–π\"\n",
    ")\n",
    "\n",
    "# –°–æ—Ö—Ä–∞–Ω—è–µ–º –¥–ª—è –¥–∞–ª—å–Ω–µ–π—à–µ–≥–æ –∏—Å–ø–æ–ª—å–∑–æ–≤–∞–Ω–∏—è\n",
    "best_mlp_model = best_mlp['model']\n",
    "best_mlp_results = best_mlp\n",
    "\n",
    "# ============================================================================\n",
    "# –í–∏–∑—É–∞–ª–∏–∑–∞—Ü–∏—è –æ–±—É—á–µ–Ω–∏—è –ª—É—á—à–µ–π –º–æ–¥–µ–ª–∏\n",
    "# ============================================================================\n",
    "\n",
    "fig, axes = plt.subplots(1, 2, figsize=(14, 5))\n",
    "\n",
    "# Loss curves\n",
    "axes[0].plot(best_mlp['train_losses'], label='Train Loss', color='skyblue')\n",
    "axes[0].plot(best_mlp['val_losses'], label='Val MSE', color='coral')\n",
    "axes[0].set_title(f'Loss Curves - {best_mlp[\"name\"]}')\n",
    "axes[0].set_xlabel('Epoch')\n",
    "axes[0].set_ylabel('Loss')\n",
    "axes[0].legend()\n",
    "axes[0].grid(True, alpha=0.3)\n",
    "\n",
    "# –ü—Ä–µ–¥—Å–∫–∞–∑–∞–Ω–∏—è vs –∏—Å—Ç–∏–Ω–Ω—ã–µ –∑–Ω–∞—á–µ–Ω–∏—è\n",
    "axes[1].scatter(val_y, best_mlp['val_pred'], alpha=0.3, color='coral')\n",
    "axes[1].plot([0, 1], [0, 1], 'r--', linewidth=2)\n",
    "axes[1].set_title(f'Val: MSE={best_mlp[\"val_mse\"]:.4f}')\n",
    "axes[1].set_xlabel('–ò—Å—Ç–∏–Ω–Ω–æ–µ –∑–Ω–∞—á–µ–Ω–∏–µ')\n",
    "axes[1].set_ylabel('–ü—Ä–µ–¥—Å–∫–∞–∑–∞–Ω–∏–µ')\n",
    "axes[1].set_xlim(0, 1)\n",
    "axes[1].set_ylim(0, 1)\n",
    "axes[1].grid(True, alpha=0.3)\n",
    "\n",
    "plt.tight_layout()\n",
    "plt.show()\n",
    "\n",
    "print(f\"\\n MLP –æ–±—É—á–µ–Ω–∞ –∏ –¥–æ–±–∞–≤–ª–µ–Ω–∞ –≤ tracker!\")"
