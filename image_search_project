    "# –ê–≥—Ä–µ–≥–∞—Ü–∏—è –æ—Ü–µ–Ω–æ–∫ —ç–∫—Å–ø–µ—Ä—Ç–æ–≤ ---\n",
    "print(\"=\" * 70)\n",
    "print(\"–ê–ì–†–ï–ì–ê–¶–ò–Ø –û–¶–ï–ù–û–ö –≠–ö–°–ü–ï–†–¢–û–í\")\n",
    "print(\"=\" * 70)\n",
    "\n",
    "# –ö–æ–ø–∏—Ä—É–µ–º –¥–∞—Ç–∞—Å–µ—Ç –¥–ª—è —Ä–∞–±–æ—Ç—ã\n",
    "df = expert_annotations.copy()\n",
    "\n",
    "# –§—É–Ω–∫—Ü–∏—è –¥–ª—è –≥–æ–ª–æ—Å–æ–≤–∞–Ω–∏—è –±–æ–ª—å—à–∏–Ω—Å—Ç–≤–∞\n",
    "def aggregate_scores(row):\n",
    "    \"\"\"\n",
    "    –ê–≥—Ä–µ–≥–∏—Ä—É–µ—Ç 3 –æ—Ü–µ–Ω–∫–∏ —ç–∫—Å–ø–µ—Ä—Ç–æ–≤ –≤ –æ–¥–Ω—É.\n",
    "    –ï—Å–ª–∏ –≤—Å–µ 3 –æ—Ü–µ–Ω–∫–∏ —Ä–∞–∑–Ω—ã–µ ‚Äî –≤–æ–∑–≤—Ä–∞—â–∞–µ—Ç None (–∑–∞–ø–∏—Å—å –±—É–¥–µ—Ç —É–¥–∞–ª–µ–Ω–∞)\n",
    "    \"\"\"\n",
    "    scores = [row['expert_1'], row['expert_2'], row['expert_3']]\n",
    "\n",
    "    # –°—á–∏—Ç–∞–µ–º –º–æ–¥—É (–Ω–∞–∏–±–æ–ª–µ–µ —á–∞—Å—Ç–æ–µ –∑–Ω–∞—á–µ–Ω–∏–µ)\n",
    "    mode_result = stats.mode(scores, keepdims=True)\n",
    "    mode_value = mode_result.mode[0]\n",
    "    mode_count = mode_result.count[0]\n",
    "\n",
    "    # –ï—Å–ª–∏ –≤—Å–µ 3 –æ—Ü–µ–Ω–∫–∏ —Ä–∞–∑–Ω—ã–µ (mode_count = 1) ‚Äî –≤–æ–∑–≤—Ä–∞—â–∞–µ–º None\n",
    "    if mode_count == 1:\n",
    "        return None\n",
    "\n",
    "    # –ò–Ω–∞—á–µ –≤–æ–∑–≤—Ä–∞—â–∞–µ–º –æ—Ü–µ–Ω–∫—É –±–æ–ª—å—à–∏–Ω—Å—Ç–≤–∞\n",
    "    return mode_value\n",
    "\n",
    "# –ü—Ä–∏–º–µ–Ω—è–µ–º –∞–≥—Ä–µ–≥–∞—Ü–∏—é\n",
    "df['target_aggregated'] = df.apply(aggregate_scores, axis=1)\n",
    "\n",
    "# –°—á–∏—Ç–∞–µ–º —Å—Ç–∞—Ç–∏—Å—Ç–∏–∫—É\n",
    "total_rows = len(df)\n",
    "rows_with_disagreement = df['target_aggregated'].isnull().sum()\n",
    "rows_kept = total_rows - rows_with_disagreement\n",
    "\n",
    "print(f\"\\n –†–µ–∑—É–ª—å—Ç–∞—Ç—ã –∞–≥—Ä–µ–≥–∞—Ü–∏–∏:\")\n",
    "print(f\"   –í—Å–µ–≥–æ –∑–∞–ø–∏—Å–µ–π: {total_rows}\")\n",
    "print(f\"   –ó–∞–ø–∏—Å–µ–π —Å –ø–æ–ª–Ω—ã–º —Ä–∞–∑–Ω–æ–≥–ª–∞—Å–∏–µ–º (—É–¥–∞–ª–µ–Ω–æ): {rows_with_disagreement} ({rows_with_disagreement/total_rows*100:.1f}%)\")\n",
    "print(f\"   –ó–∞–ø–∏—Å–µ–π –¥–ª—è –æ–±—É—á–µ–Ω–∏—è (–æ—Å—Ç–∞–ª–æ—Å—å): {rows_kept} ({rows_kept/total_rows*100:.1f}%)\")\n",
    "\n",
