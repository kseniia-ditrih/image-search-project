    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "\n",
      " CLIP —Å –ø–æ–¥–±–æ—Ä–æ–º –≥–∏–ø–µ—Ä–ø–∞—Ä–∞–º–µ—Ç—Ä–æ–≤ –∑–∞–≤–µ—Ä—à–µ–Ω–∞!\n"
     ]
    }
   ],
   "source": [
    "print(\"=\" * 70)\n",
    "print(\"–û–¶–ï–ù–ö–ê –ú–û–î–ï–õ–ò CLIP –° –ü–û–î–ë–û–†–û–ú –ì–ò–ü–ï–†–ü–ê–†–ê–ú–ï–¢–†–û–í\")\n",
    "print(\"=\" * 70)\n",
    "\n",
    "# ============================================================================\n",
    "# 1. –£—Å—Ç–∞–Ω–æ–≤–∫–∞ –ø—Ä–∞–≤–∏–ª—å–Ω–æ–≥–æ CLIP (–µ—Å–ª–∏ –µ—â—ë –Ω–µ —É—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω)\n",
    "# ============================================================================\n",
    "\n",
    "try:\n",
    "    import clip\n",
    "    print(f\"CLIP —É–∂–µ —É—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω: {clip.__file__}\")\n",
    "except ImportError:\n",
    "    print(\"üì• –£—Å—Ç–∞–Ω–æ–≤–∫–∞ –æ—Ñ–∏—Ü–∏–∞–ª—å–Ω–æ–≥–æ CLIP –æ—Ç OpenAI...\")\n",
    "    subprocess.check_call([sys.executable, \"-m\", \"pip\", \"uninstall\", \"clip\", \"-y\"])\n",
    "    subprocess.check_call([sys.executable, \"-m\", \"pip\", \"install\",\n",
    "                          \"git+https://github.com/openai/CLIP.git\"])\n",
    "    import clip\n",
    "    print(\"CLIP —É—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω!\")\n",
    "\n",
    "device = torch.device('cuda' if torch.cuda.is_available() else 'cpu')\n",
    "print(f\"–£—Å—Ç—Ä–æ–π—Å—Ç–≤–æ: {device}\")\n",
    "\n",
    "# ============================================================================\n",
    "# 2. –§—É–Ω–∫—Ü–∏–∏ –¥–ª—è –≤–µ–∫—Ç–æ—Ä–∏–∑–∞—Ü–∏–∏ –∏ –æ—Ü–µ–Ω–∫–∏\n",
    "# ============================================================================\n",
    "\n",
    "def get_clip_embeddings(df, images_path, model, preprocess, device):\n",
    "    \"\"\"–í–µ–∫—Ç–æ—Ä–∏–∑—É–µ—Ç –∏–∑–æ–±—Ä–∞–∂–µ–Ω–∏—è –∏ —Ç–µ–∫—Å—Ç—ã —á–µ—Ä–µ–∑ CLIP —Å –∫—ç—à–∏—Ä–æ–≤–∞–Ω–∏–µ–º\"\"\"\n",
    "\n",
    "    image_cache = {}\n",
    "    image_embeddings = []\n",
    "    text_embeddings = []\n",
    "\n",
    "    # –ò–∑–æ–±—Ä–∞–∂–µ–Ω–∏—è\n",
    "    for idx, row in tqdm(df.iterrows(), total=len(df), desc=\"–ò–∑–æ–±—Ä–∞–∂–µ–Ω–∏—è\", leave=False):\n",
    "        img_name = row['image']\n",
    "        img_path = os.path.join(images_path, img_name)\n",
    "\n",
    "        if img_name not in image_cache:\n",
    "            try:\n",
    "                image = Image.open(img_path).convert('RGB')\n",
    "                image_tensor = preprocess(image).unsqueeze(0).to(device)\n",
    "\n",
    "                with torch.no_grad():\n",
    "                    embedding = model.encode_image(image_tensor)\n",
    "                    embedding = embedding / embedding.norm(dim=-1, keepdim=True)\n",
    "\n",
    "                image_cache[img_name] = embedding.squeeze().cpu().numpy()\n",
    "            except Exception as e:\n",
    "                print(f\" –û—à–∏–±–∫–∞ –∑–∞–≥—Ä—É–∑–∫–∏ {img_name}: {e}\")\n",
    "                image_cache[img_name] = np.zeros(512)\n",
    "\n",
    "        image_embeddings.append(image_cache[img_name])\n",
    "\n",
    "    # –¢–µ–∫—Å—Ç—ã\n",
    "    for idx, text in tqdm(enumerate(df['query_text']), total=len(df), desc=\"–¢–µ–∫—Å—Ç—ã\", leave=False):\n",
    "        try:\n",
    "            tokens = clip.tokenize([str(text)], truncate=True).to(device)\n",
    "\n",
    "            with torch.no_grad():\n",
    "                embedding = model.encode_text(tokens)\n",
    "                embedding = embedding / embedding.norm(dim=-1, keepdim=True)\n",
    "\n",
    "            text_embeddings.append(embedding.squeeze().cpu().numpy())\n",
    "        except Exception as e:\n",
    "            print(f\" –û—à–∏–±–∫–∞ —Ç–æ–∫–µ–Ω–∏–∑–∞—Ü–∏–∏: {e}\")\n",
    "            text_embeddings.append(np.zeros(512))\n",
    "\n",
    "    return np.array(image_embeddings), np.array(text_embeddings)\n",
    "\n",
    "def evaluate_clip_config(model_name, calibration_method='none'):\n",
    "    \"\"\"–û—Ü–µ–Ω–∏–≤–∞–µ—Ç –æ–¥–Ω—É –∫–æ–Ω—Ñ–∏–≥—É—Ä–∞—Ü–∏—é CLIP\"\"\"\n",
    "\n",
    "    print(f\"\\n –¢–µ—Å—Ç–∏—Ä—É–µ–º: {model_name} | –ö–∞–ª–∏–±—Ä–æ–≤–∫–∞: {calibration_method}\")\n",
    "\n",
    "    # –ó–∞–≥—Ä—É–∑–∫–∞ –º–æ–¥–µ–ª–∏\n",
    "    clip_model, clip_preprocess = clip.load(model_name, device=device)\n",
    "    clip_model.eval()\n",
    "\n",
    "    # –í–µ–∫—Ç–æ—Ä–∏–∑–∞—Ü–∏—è\n",
    "    train_img_emb, train_txt_emb = get_clip_embeddings(\n",
    "        train_df, TRAIN_IMAGES_PATH, clip_model, clip_preprocess, device\n",
    "    )\n",
    "    val_img_emb, val_txt_emb = get_clip_embeddings(\n",
    "        val_df, TRAIN_IMAGES_PATH, clip_model, clip_preprocess, device\n",
    "    )\n",
    "\n",
    "    # –°—ã—Ä–∞—è —Å—Ö–æ–∂–µ—Å—Ç—å (–∫–æ—Å–∏–Ω—É—Å–Ω–æ–µ —Ä–∞—Å—Å—Ç–æ—è–Ω–∏–µ)\n",
    "    train_raw_sim = np.sum(train_img_emb * train_txt_emb, axis=1)\n",
    "    val_raw_sim = np.sum(val_img_emb * val_txt_emb, axis=1)\n",
    "\n",
    "    # –ö–∞–ª–∏–±—Ä–æ–≤–∫–∞\n",
    "    if calibration_method == 'none':\n",
    "        train_pred = (train_raw_sim + 1) / 2\n",
    "        val_pred = (val_raw_sim + 1) / 2\n",
    "\n",
    "    elif calibration_method == 'linear':\n",
    "        calibrator = LinearRegression()\n",
    "        calibrator.fit(train_raw_sim.reshape(-1, 1), train_y)\n",
    "        train_pred = calibrator.predict(train_raw_sim.reshape(-1, 1))\n",
    "        val_pred = calibrator.predict(val_raw_sim.reshape(-1, 1))\n",
    "\n",
    "    elif calibration_method == 'ridge':\n",
    "        calibrator = Ridge(alpha=1.0)\n",
    "        calibrator.fit(train_raw_sim.reshape(-1, 1), train_y)\n",
    "        train_pred = calibrator.predict(train_raw_sim.reshape(-1, 1))\n",
    "        val_pred = calibrator.predict(val_raw_sim.reshape(-1, 1))\n",
    "\n",
    "    elif calibration_method == 'minmax':\n",
    "        sim_min, sim_max = train_raw_sim.min(), train_raw_sim.max()\n",
    "        train_pred = (train_raw_sim - sim_min) / (sim_max - sim_min)\n",
    "        val_pred = (val_raw_sim - sim_min) / (sim_max - sim_min)\n",
    "\n",
    "    else:\n",
    "        raise ValueError(f\"–ù–µ–∏–∑–≤–µ—Å—Ç–Ω—ã–π –º–µ—Ç–æ–¥ –∫–∞–ª–∏–±—Ä–æ–≤–∫–∏: {calibration_method}\")\n",
    "\n",
    "    # –û–≥—Ä–∞–Ω–∏—á–µ–Ω–∏–µ [0, 1]\n",
    "    train_pred = np.clip(train_pred, 0, 1)\n",
    "    val_pred = np.clip(val_pred, 0, 1)\n",
    "\n",
    "    # –ú–µ—Ç—Ä–∏–∫–∏\n",
    "    train_mse = mean_squared_error(train_y, train_pred)\n",
    "    val_mse = mean_squared_error(val_y, val_pred)\n",
    "    train_mae = mean_absolute_error(train_y, train_pred)\n",
    "    val_mae = mean_absolute_error(val_y, val_pred)\n",
    "\n",
    "    print(f\"   Val MSE: {val_mse:.6f} | Val MAE: {val_mae:.6f}\")\n",
    "\n",
    "    return {\n",
    "        'model_name': model_name,\n",
    "        'calibration': calibration_method,\n",
    "        'val_mse': val_mse,\n",
    "        'val_mae': val_mae,\n",
    "        'train_mse': train_mse,\n",
    "        'train_mae': train_mae,\n",
    "        'val_pred': val_pred\n",
    "    }\n",
    "\n",
    "# ============================================================================\n",
    "# 3. –ü–æ–¥–±–æ—Ä –≥–∏–ø–µ—Ä–ø–∞—Ä–∞–º–µ—Ç—Ä–æ–≤\n",
    "# ============================================================================\n",
    "\n",
    "print(f\"\\n{'=' * 70}\")\n",
    "print(f\" –ü–û–î–ë–û–† –ì–ò–ü–ï–†–ü–ê–†–ê–ú–ï–¢–†–û–í CLIP\")\n",
    "print(f\"{'=' * 70}\")\n",
    "print(f\"–¢–µ—Å—Ç–∏—Ä—É–µ–º—ã–µ –∞—Ä—Ö–∏—Ç–µ–∫—Ç—É—Ä—ã: ViT-B/32, ViT-B/16, RN50\")\n",
    "print(f\"–ú–µ—Ç–æ–¥—ã –∫–∞–ª–∏–±—Ä–æ–≤–∫–∏: none, linear, ridge, minmax\")\n",
    "\n",
    "configs = [\n",
    "    {'model': 'ViT-B/32', 'calibration': 'none'},\n",
    "    {'model': 'ViT-B/32', 'calibration': 'linear'},\n",
    "    {'model': 'ViT-B/32', 'calibration': 'ridge'},\n",
    "    {'model': 'ViT-B/32', 'calibration': 'minmax'},\n",
    "    {'model': 'ViT-B/16', 'calibration': 'none'},\n",
    "    {'model': 'ViT-B/16', 'calibration': 'linear'},\n",
    "    {'model': 'RN50', 'calibration': 'none'},\n",
    "    {'model': 'RN50', 'calibration': 'linear'},\n",
    "]\n",
    "\n",
    "clip_results = []\n",
    "start_time_total = time.time()\n",
    "\n",
    "for i, config in enumerate(configs, 1):\n",
    "    try:\n",
    "        result = evaluate_clip_config(\n",
    "            model_name=config['model'],\n",
    "            calibration_method=config['calibration']\n",
    "        )\n",
    "        result['full_name'] = f\"CLIP ({config['model']}) + {config['calibration']}\"\n",
    "        result['hyperparams'] = f\"model={config['model']}, calibration={config['calibration']}\"\n",
    "        clip_results.append(result)\n",
    "    except Exception as e:\n",
    "        print(f\" –û—à–∏–±–∫–∞ –≤ –∫–æ–Ω—Ñ–∏–≥—É—Ä–∞—Ü–∏–∏ {i}: {e}\")\n",
    "        continue\n",
    "\n",
    "total_time = time.time() - start_time_total\n",
    "print(f\"\\n –û–±—â–µ–µ –≤—Ä–µ–º—è –ø–æ–¥–±–æ—Ä–∞: {total_time:.1f} —Å–µ–∫\")\n",
    "\n",
    "# ============================================================================\n",
    "# 4. –í—ã–±–æ—Ä –ª—É—á—à–µ–π –∫–æ–Ω—Ñ–∏–≥—É—Ä–∞—Ü–∏–∏\n",
    "# ============================================================================\n",
    "\n",
    "best_clip = min(clip_results, key=lambda x: x['val_mse'])\n",
    "\n",
    "print(f\"\\n{'=' * 70}\")\n",
    "print(f\" –õ–£–ß–®–ê–Ø –ö–û–ù–§–ò–ì–£–†–ê–¶–ò–Ø CLIP\")\n",
    "print(f\"{'=' * 70}\")\n",
    "print(f\"   –ú–æ–¥–µ–ª—å: {best_clip['full_name']}\")\n",
    "print(f\"   Val MSE: {best_clip['val_mse']:.6f}\")\n",
    "print(f\"   Val MAE: {best_clip['val_mae']:.6f}\")\n",
    "print(f\"   –ì–∏–ø–µ—Ä–ø–∞—Ä–∞–º–µ—Ç—Ä—ã: {best_clip['hyperparams']}\")\n",
    "\n",
    "# –î–æ–±–∞–≤–ª—è–µ–º –≤ —Ç—Ä–µ–∫–µ—Ä\n",
    "tracker.add_model(\n",
    "    model_name=f\"CLIP Best - {best_clip['full_name']}\",\n",
    "    model_obj=None,\n",
    "    train_metrics={'mse': best_clip['train_mse'], 'mae': best_clip['train_mae']},\n",
    "    val_metrics={'mse': best_clip['val_mse'], 'mae': best_clip['val_mae']},\n",
    "    hyperparams=best_clip['hyperparams'],\n",
    "    train_time=f\"{total_time:.1f} —Å–µ–∫\",\n",
    "    notes=\"CLIP —Å –ø–æ–¥–±–æ—Ä–æ–º –∞—Ä—Ö–∏—Ç–µ–∫—Ç—É—Ä—ã –∏ –∫–∞–ª–∏–±—Ä–æ–≤–∫–æ–π\"\n",
    ")\n",
    "\n",
    "# ============================================================================\n",
    "# 5. –í–∏–∑—É–∞–ª–∏–∑–∞—Ü–∏—è —Ä–µ–∑—É–ª—å—Ç–∞—Ç–æ–≤ –ø–æ–¥–±–æ—Ä–∞\n",
    "# ============================================================================\n",
    "\n",
    "print(f\"\\n –°–†–ê–í–ù–ï–ù–ò–ï –ö–û–ù–§–ò–ì–£–†–ê–¶–ò–ô CLIP\")\n",
    "\n",
    "results_df = pd.DataFrame([\n",
    "    {\n",
    "        '–ê—Ä—Ö–∏—Ç–µ–∫—Ç—É—Ä–∞': r['model_name'],\n",
    "        '–ö–∞–ª–∏–±—Ä–æ–≤–∫–∞': r['calibration'],\n",
    "        'Val MSE': r['val_mse'],\n",
    "        'Val MAE': r['val_mae']\n",
    "    }\n",
    "    for r in clip_results\n",
    "]).sort_values('Val MSE')\n",
    "\n",
    "display(results_df)\n",
    "\n",
    "# –ì—Ä–∞—Ñ–∏–∫ —Å—Ä–∞–≤–Ω–µ–Ω–∏—è\n",
    "fig, ax = plt.subplots(figsize=(10, 6))\n",
    "bars = ax.barh(\n",
    "    [f\"{r['model_name']} + {r['calibration']}\" for r in clip_results],\n",
    "    [r['val_mse'] for r in clip_results],\n",
    "    color=['gold' if r == best_clip else 'skyblue' for r in clip_results]\n",
    ")\n",
    "ax.set_xlabel('Val MSE (–º–µ–Ω—å—à–µ ‚Äî –ª—É—á—à–µ)')\n",
    "ax.set_title('–°—Ä–∞–≤–Ω–µ–Ω–∏–µ –∫–æ–Ω—Ñ–∏–≥—É—Ä–∞—Ü–∏–π CLIP')\n",
    "ax.invert_yaxis()\n",
    "plt.tight_layout()\n",
    "plt.show()\n",
    "\n",
    "print(f\"\\n CLIP —Å –ø–æ–¥–±–æ—Ä–æ–º –≥–∏–ø–µ—Ä–ø–∞—Ä–∞–º–µ—Ç—Ä–æ–≤ –∑–∞–≤–µ—Ä—à–µ–Ω–∞!\")"
   ]
