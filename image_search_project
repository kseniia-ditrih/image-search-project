    "print(\"–í–ï–ö–¢–û–†–ò–ó–ê–¶–ò–Ø –¢–ï–ö–°–¢–û–í (BERT)\")\n",
    "print(\"=\" * 70)\n",
    "\n",
    "# –ò—Å–ø–æ–ª—å–∑—É–µ–º distilbert ‚Äî –±—ã—Å—Ç—Ä–µ–µ –∏ –ª–µ–≥—á–µ, —á–µ–º full BERT\n",
    "MODEL_NAME = 'distilbert-base-uncased'\n",
    "\n",
    "print(f\"\\n –ó–∞–≥—Ä—É–∂–∞–µ–º —Ç–æ–∫–µ–Ω–∏–∑–∞—Ç–æ—Ä –∏ –º–æ–¥–µ–ª—å {MODEL_NAME}...\")\n",
    "tokenizer = BertTokenizer.from_pretrained(MODEL_NAME)\n",
    "bert_model = BertModel.from_pretrained(MODEL_NAME)\n",
    "bert_model = bert_model.to(device)\n",
    "bert_model.eval()\n",
    "\n",
    "print(f\" –ú–æ–¥–µ–ª—å –∑–∞–≥—Ä—É–∂–µ–Ω–∞ –Ω–∞ —É—Å—Ç—Ä–æ–π—Å—Ç–≤–æ: {device}\")\n",
    "\n",
    "# –§—É–Ω–∫—Ü–∏—è –¥–ª—è –ø–æ–ª—É—á–µ–Ω–∏—è —Ç–µ–∫—Å—Ç–æ–≤–æ–≥–æ —ç–º–±–µ–¥–¥–∏–Ω–≥–∞\n",
    "def get_text_embedding(text, tokenizer, model, device, max_length=128):\n",
    "    \"\"\"–í–æ–∑–≤—Ä–∞—â–∞–µ—Ç –≤–µ–∫—Ç–æ—Ä–Ω–æ–µ –ø—Ä–µ–¥—Å—Ç–∞–≤–ª–µ–Ω–∏–µ —Ç–µ–∫—Å—Ç–∞ (768 dim –¥–ª—è DistilBERT)\"\"\"\n",
    "    if pd.isna(text):\n",
    "        text = \"\"\n",
    "\n",
    "    # –¢–æ–∫–µ–Ω–∏–∑–∞—Ü–∏—è\n",
    "    inputs = tokenizer(\n",
    "        str(text),\n",
    "        return_tensors='pt',\n",
    "        padding=True,\n",
    "        truncation=True,\n",
    "        max_length=max_length\n",
    "    ).to(device)\n",
    "\n",
    "    with torch.no_grad():\n",
    "        outputs = model(**inputs)\n",
    "\n",
    "    # –ë–µ—Ä—ë–º [CLS] —Ç–æ–∫–µ–Ω –∫–∞–∫ –ø—Ä–µ–¥—Å—Ç–∞–≤–ª–µ–Ω–∏–µ –≤—Å–µ–≥–æ —Ç–µ–∫—Å—Ç–∞\n",
    "    cls_embedding = outputs.last_hidden_state[:, 0, :]\n",
    "\n",
    "    return cls_embedding.squeeze().cpu().numpy()\n",
    "\n",
    "# –í–µ–∫—Ç–æ—Ä–∏–∑—É–µ–º —Ç–µ–∫—Å—Ç—ã –¥–ª—è train\n",
    "print(f\"\\n –í–µ–∫—Ç–æ—Ä–∏–∑—É–µ–º —Ç–µ–∫—Å—Ç—ã –¥–ª—è train ({len(train_df)} –∑–∞–ø–∏—Å–µ–π)...\")\n",
    "\n",
    "train_text_embeddings = []\n",
    "for idx, text in tqdm(enumerate(train_df['query_text']), total=len(train_df)):\n",
    "    embedding = get_text_embedding(text, tokenizer, bert_model, device)\n",
    "    train_text_embeddings.append(embedding)\n",
    "\n",
    "# –í–µ–∫—Ç–æ—Ä–∏–∑—É–µ–º —Ç–µ–∫—Å—Ç—ã –¥–ª—è val\n",
    "print(f\"\\n –í–µ–∫—Ç–æ—Ä–∏–∑—É–µ–º —Ç–µ–∫—Å—Ç—ã –¥–ª—è val ({len(val_df)} –∑–∞–ø–∏—Å–µ–π)...\")\n",
    "\n",
    "val_text_embeddings = []\n",
    "for idx, text in tqdm(enumerate(val_df['query_text']), total=len(val_df)):\n",
    "    embedding = get_text_embedding(text, tokenizer, bert_model, device)\n",
    "    val_text_embeddings.append(embedding)\n",
    "\n",
    "# –ü—Ä–µ–æ–±—Ä–∞–∑—É–µ–º –≤ numpy –º–∞—Å—Å–∏–≤—ã\n",
    "train_text_embeddings = np.array(train_text_embeddings)\n",
    "val_text_embeddings = np.array(val_text_embeddings)\n",
    "\n",
    "print(f\"\\n –í–µ–∫—Ç–æ—Ä–∏–∑–∞—Ü–∏—è —Ç–µ–∫—Å—Ç–æ–≤ –∑–∞–≤–µ—Ä—à–µ–Ω–∞!\")\n",
    "print(f\"   Train embeddings: {train_text_embeddings.shape}\")\n",
    "print(f\"   Val embeddings: {val_text_embeddings.shape}\")\n",
    "print(f\"   –†–∞–∑–º–µ—Ä–Ω–æ—Å—Ç—å –≤–µ–∫—Ç–æ—Ä–∞: {train_text_embeddings.shape[1]} (DistilBERT output)\")\n",
    "\n",
    "# –û—Å–≤–æ–±–æ–∂–¥–∞–µ–º –ø–∞–º—è—Ç—å\n",
