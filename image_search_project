   "source": [
    "# –°–∏—Å—Ç–µ–º–∞ –¥–ª—è –∞–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–æ–≥–æ —Å–±–æ—Ä–∞ —Ä–µ–∑—É–ª—å—Ç–∞—Ç–æ–≤\n",
    "class ModelResultsTracker:\n",
    "    \"\"\"–ö–ª–∞—Å—Å –¥–ª—è –∞–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–æ–≥–æ —Å–±–æ—Ä–∞ –∏ —Å—Ä–∞–≤–Ω–µ–Ω–∏—è —Ä–µ–∑—É–ª—å—Ç–∞—Ç–æ–≤ –º–æ–¥–µ–ª–µ–π\"\"\"\n",
    "\n",
    "    def __init__(self):\n",
    "        self.results = []\n",
    "\n",
    "    def add_model(self, model_name, model_obj, train_metrics, val_metrics,\n",
    "                  hyperparams=None, train_time=None, notes=\"\"):\n",
    "        \"\"\"–î–æ–±–∞–≤–ª—è–µ—Ç —Ä–µ–∑—É–ª—å—Ç–∞—Ç—ã –º–æ–¥–µ–ª–∏ –≤ —Ç–∞–±–ª–∏—Ü—É\"\"\"\n",
    "\n",
    "        result = {\n",
    "            '–ú–æ–¥–µ–ª—å': model_name,\n",
    "            'Val MSE': val_metrics.get('mse', None),\n",
    "            'Val MAE': val_metrics.get('mae', None),\n",
    "            'Val RMSE': val_metrics.get('rmse', None),\n",
    "            'Train MSE': train_metrics.get('mse', None),\n",
    "            'Train MAE': train_metrics.get('mae', None),\n",
    "            '–ì–∏–ø–µ—Ä–ø–∞—Ä–∞–º–µ—Ç—Ä—ã': hyperparams if hyperparams else \"‚Äî\",\n",
    "            '–í—Ä–µ–º—è –æ–±—É—á–µ–Ω–∏—è': train_time if train_time else \"‚Äî\",\n",
    "            '–ó–∞–º–µ—Ç–∫–∏': notes,\n",
    "            '–î–∞—Ç–∞': datetime.now().strftime(\"%H:%M:%S\"),\n",
    "            'model_obj': model_obj  # –°–æ—Ö—Ä–∞–Ω—è–µ–º —Å–∞–º—É –º–æ–¥–µ–ª—å –¥–ª—è –¥–∞–ª—å–Ω–µ–π—à–µ–≥–æ –∏—Å–ø–æ–ª—å–∑–æ–≤–∞–Ω–∏—è\n",
    "        }\n",
    "\n",
    "        self.results.append(result)\n",
    "        print(f\" –î–æ–±–∞–≤–ª–µ–Ω–∞ –º–æ–¥–µ–ª—å: {model_name} | Val MSE: {val_metrics.get('mse', 'N/A'):.6f}\")\n",
    "\n",
    "    def get_comparison_table(self, sort_by='Val MSE', ascending=True):\n",
    "        \"\"\"–í–æ–∑–≤—Ä–∞—â–∞–µ—Ç —Ç–∞–±–ª–∏—Ü—É —Å—Ä–∞–≤–Ω–µ–Ω–∏—è –≤—Å–µ—Ö –º–æ–¥–µ–ª–µ–π\"\"\"\n",
    "\n",
    "        df = pd.DataFrame(self.results)\n",
    "\n",
    "        # –°–æ—Ä—Ç–∏—Ä—É–µ–º –ø–æ –ª—É—á—à–µ–π –º–µ—Ç—Ä–∏–∫–µ\n",
    "        if sort_by in df.columns:\n",
    "            df = df.sort_values(by=sort_by, ascending=ascending)\n",
    "\n",
    "        # –í—ã–¥–µ–ª—è–µ–º –ª—É—á—à—É—é –º–æ–¥–µ–ª—å\n",
    "        if len(df) > 0:\n",
    "            best_idx = df[sort_by].idxmin() if ascending else df[sort_by].idxmax()\n",
    "            df.loc[best_idx, '–ú–æ–¥–µ–ª—å'] = f\"üèÜ {df.loc[best_idx, '–ú–æ–¥–µ–ª—å']}\"\n",
    "\n",
    "        # –í–æ–∑–≤—Ä–∞—â–∞–µ–º —Ç–∞–±–ª–∏—Ü—É –±–µ–∑ —Å–ª—É–∂–µ–±–Ω—ã—Ö –∫–æ–ª–æ–Ω–æ–∫\n",
    "        display_cols = ['–ú–æ–¥–µ–ª—å', 'Val MSE', 'Val MAE', 'Val RMSE',\n",
    "                       '–ì–∏–ø–µ—Ä–ø–∞—Ä–∞–º–µ—Ç—Ä—ã', '–í—Ä–µ–º—è –æ–±—É—á–µ–Ω–∏—è', '–ó–∞–º–µ—Ç–∫–∏']\n",
    "        return df[display_cols].reset_index(drop=True)\n",
    "\n",
    "    def get_best_model(self, metric='Val MSE'):\n",
    "        \"\"\"–í–æ–∑–≤—Ä–∞—â–∞–µ—Ç –ª—É—á—à—É—é –º–æ–¥–µ–ª—å –ø–æ —É–∫–∞–∑–∞–Ω–Ω–æ–π –º–µ—Ç—Ä–∏–∫–µ\"\"\"\n",
    "\n",
    "        if not self.results:\n",
    "            return None\n",
    "\n",
    "        df = pd.DataFrame(self.results)\n",
    "        best_idx = df[metric].idxmin()\n",
    "\n",
    "        return {\n",
    "            'name': df.loc[best_idx, '–ú–æ–¥–µ–ª—å'],\n",
    "            'model': df.loc[best_idx, 'model_obj'],\n",
    "            'val_mse': df.loc[best_idx, 'Val MSE'],\n",
    "            'val_mae': df.loc[best_idx, 'Val MAE']\n",
    "        }\n",
    "\n",
    "    def plot_comparison(self, metric='Val MSE'):\n",
    "        \"\"\"–í–∏–∑—É–∞–ª–∏–∑–∏—Ä—É–µ—Ç —Å—Ä–∞–≤–Ω–µ–Ω–∏–µ –º–æ–¥–µ–ª–µ–π\"\"\"\n",
    "\n",
    "        df = pd.DataFrame(self.results)\n",
    "\n",
    "        if len(df) == 0:\n",
    "            print(\"–ù–µ—Ç –¥–∞–Ω–Ω—ã—Ö –¥–ª—è –≤–∏–∑—É–∞–ª–∏–∑–∞—Ü–∏–∏\")\n",
    "            return\n",
    "\n",
    "        fig, ax = plt.subplots(figsize=(12, 6))\n",
    "\n",
    "        # –°—Ç–æ–ª–±—á–∞—Ç–∞—è –¥–∏–∞–≥—Ä–∞–º–º–∞\n",
    "        bars = ax.barh(df['–ú–æ–¥–µ–ª—å'], df[metric], color='skyblue')\n",
    "\n",
    "        # –ü–æ–¥–ø–∏—Å—ã–≤–∞–µ–º –∑–Ω–∞—á–µ–Ω–∏—è\n",
    "        for i, v in enumerate(df[metric]):\n",
    "            ax.text(v + 0.001, i, f'{v:.4f}', va='center', fontsize=10)\n",
    "\n",
    "        ax.set_xlabel(metric)\n",
    "        ax.set_title(f'–°—Ä–∞–≤–Ω–µ–Ω–∏–µ –º–æ–¥–µ–ª–µ–π –ø–æ {metric}')\n",
    "        ax.invert_yaxis()  # –õ—É—á—à–∞—è –º–æ–¥–µ–ª—å —Å–≤–µ—Ä—Ö—É\n",
    "        plt.grid(axis='x', alpha=0.3)\n",
    "        plt.tight_layout()\n",
    "        plt.show()\n",
    "\n",
    "# –°–æ–∑–¥–∞—ë–º —Ç—Ä–µ–∫–µ—Ä\n",
    "tracker = ModelResultsTracker()\n",
    "\n",
    "print(\" –°–∏—Å—Ç–µ–º–∞ –¥–ª—è —Å–±–æ—Ä–∞ —Ä–µ–∑—É–ª—å—Ç–∞—Ç–æ–≤ –≥–æ—Ç–æ–≤–∞!\")\n",
    "print(\"   –ò—Å–ø–æ–ª—å–∑—É–π—Ç–µ: tracker.add_model(...) –¥–ª—è –¥–æ–±–∞–≤–ª–µ–Ω–∏—è —Ä–µ–∑—É–ª—å—Ç–∞—Ç–æ–≤\")\n",
    "print(\"   –ò—Å–ø–æ–ª—å–∑—É–π—Ç–µ: tracker.get_comparison_table() –¥–ª—è –≤—ã–≤–æ–¥–∞ —Ç–∞–±–ª–∏—Ü—ã\")"
   ]
