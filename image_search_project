    "\n",
    "# –ü—Ä–æ–≤–µ—Ä—è–µ–º –¥–æ—Å—Ç—É–ø–Ω–æ—Å—Ç—å GPU\n",
    "device = torch.device('cuda' if torch.cuda.is_available() else 'cpu')\n",
    "print(f\"\\n –ò—Å–ø–æ–ª—å–∑—É–µ–º–æ–µ —É—Å—Ç—Ä–æ–π—Å—Ç–≤–æ: {device}\")\n",
    "\n",
    "# –ó–∞–≥—Ä—É–∂–∞–µ–º –ø—Ä–µ–¥–æ–±—É—á–µ–Ω–Ω—É—é ResNet50\n",
    "print(f\"\\n –ó–∞–≥—Ä—É–∂–∞–µ–º ResNet50 (pretrained –Ω–∞ ImageNet)...\")\n",
    "resnet = models.resnet50(weights=models.ResNet50_Weights.IMAGENET1K_V1)\n",
    "\n",
    "# –£–¥–∞–ª—è–µ–º –ø–æ—Å–ª–µ–¥–Ω–∏–π –ø–æ–ª–Ω–æ—Å–≤—è–∑–Ω—ã–π —Å–ª–æ–π (classification head)\n",
    "# –û—Å—Ç–∞–≤–ª—è–µ–º —Ç–æ–ª—å–∫–æ feature extractor\n",
    "resnet = nn.Sequential(*list(resnet.children())[:-1])\n",
    "resnet = resnet.to(device)\n",
    "resnet.eval()\n",
    "\n",
    "# –ü—Ä–µ–æ–±—Ä–∞–∑–æ–≤–∞–Ω–∏—è –¥–ª—è –∏–∑–æ–±—Ä–∞–∂–µ–Ω–∏–π (—Ç—Ä–µ–±–æ–≤–∞–Ω–∏—è ResNet50)\n",
    "image_transform = transforms.Compose([\n",
    "    transforms.Resize((224, 224)),\n",
    "    transforms.ToTensor(),\n",
    "    transforms.Normalize(\n",
    "        mean=[0.485, 0.456, 0.406],\n",
    "        std=[0.229, 0.224, 0.225]\n",
    "    )\n",
    "])\n",
    "\n",
    "# –§—É–Ω–∫—Ü–∏—è –¥–ª—è –ø–æ–ª—É—á–µ–Ω–∏—è —ç–º–±–µ–¥–¥–∏–Ω–≥–∞ –∏–∑–æ–±—Ä–∞–∂–µ–Ω–∏—è\n",
    "def get_image_embedding(img_path, transform, model, device):\n",
    "    \"\"\"–í–æ–∑–≤—Ä–∞—â–∞–µ—Ç –≤–µ–∫—Ç–æ—Ä–Ω–æ–µ –ø—Ä–µ–¥—Å—Ç–∞–≤–ª–µ–Ω–∏–µ –∏–∑–æ–±—Ä–∞–∂–µ–Ω–∏—è (2048 dim)\"\"\"\n",
    "    try:\n",
    "        image = Image.open(img_path).convert('RGB')\n",
    "        image_tensor = transform(image).unsqueeze(0).to(device)\n",
    "\n",
    "        with torch.no_grad():\n",
    "            embedding = model(image_tensor)\n",
    "\n",
    "        return embedding.squeeze().cpu().numpy()\n",
    "    except Exception as e:\n",
    "        print(f\"–û—à–∏–±–∫–∞ –ø—Ä–∏ –∑–∞–≥—Ä—É–∑–∫–µ {img_path}: {e}\")\n",
    "        return None\n",
    "\n",
    "# –í–µ–∫—Ç–æ—Ä–∏–∑—É–µ–º –∏–∑–æ–±—Ä–∞–∂–µ–Ω–∏—è –¥–ª—è train\n",
    "print(f\"\\n –í–µ–∫—Ç–æ—Ä–∏–∑—É–µ–º –∏–∑–æ–±—Ä–∞–∂–µ–Ω–∏—è –¥–ª—è train ({len(train_df)} –∑–∞–ø–∏—Å–µ–π)...\")\n",
    "\n",
    "# –ö—ç—à–∏—Ä—É–µ–º —ç–º–±–µ–¥–¥–∏–Ω–≥–∏ (—á—Ç–æ–±—ã –Ω–µ –æ–±—Ä–∞–±–∞—Ç—ã–≤–∞—Ç—å –æ–¥–Ω–æ –∏–∑–æ–±—Ä–∞–∂–µ–Ω–∏–µ –º–Ω–æ–≥–æ —Ä–∞–∑)\n",
    "image_embeddings_cache = {}\n",
    "\n",
    "train_image_embeddings = []\n",
    "for idx, row in tqdm(train_df.iterrows(), total=len(train_df)):\n",
    "    img_name = row['image']\n",
    "    img_path = os.path.join(TRAIN_IMAGES_PATH, img_name)\n",
    "\n",
    "    # –ï—Å–ª–∏ —É–∂–µ –æ–±—Ä–∞–±–∞—Ç—ã–≤–∞–ª–∏ —ç—Ç–æ –∏–∑–æ–±—Ä–∞–∂–µ–Ω–∏–µ ‚Äî –±–µ—Ä—ë–º –∏–∑ –∫—ç—à–∞\n",
    "    if img_name not in image_embeddings_cache:\n",
    "        embedding = get_image_embedding(img_path, image_transform, resnet, device)\n",
    "        image_embeddings_cache[img_name] = embedding\n",
    "    else:\n",
    "        embedding = image_embeddings_cache[img_name]\n",
    "\n",
    "    train_image_embeddings.append(embedding)\n",
    "\n",
    "# –í–µ–∫—Ç–æ—Ä–∏–∑—É–µ–º –∏–∑–æ–±—Ä–∞–∂–µ–Ω–∏—è –¥–ª—è val\n",
    "print(f\"\\n –í–µ–∫—Ç–æ—Ä–∏–∑—É–µ–º –∏–∑–æ–±—Ä–∞–∂–µ–Ω–∏—è –¥–ª—è val ({len(val_df)} –∑–∞–ø–∏—Å–µ–π)...\")\n",
    "\n",
    "val_image_embeddings = []\n",
    "for idx, row in tqdm(val_df.iterrows(), total=len(val_df)):\n",
    "    img_name = row['image']\n",
    "    img_path = os.path.join(TRAIN_IMAGES_PATH, img_name)\n",
    "\n",
    "    if img_name not in image_embeddings_cache:\n",
    "        embedding = get_image_embedding(img_path, image_transform, resnet, device)\n",
    "        image_embeddings_cache[img_name] = embedding\n",
    "    else:\n",
    "        embedding = image_embeddings_cache[img_name]\n",
    "\n",
    "    val_image_embeddings.append(embedding)\n",
    "\n",
    "# –ü—Ä–µ–æ–±—Ä–∞–∑—É–µ–º –≤ numpy –º–∞—Å—Å–∏–≤—ã\n",
    "train_image_embeddings = np.array(train_image_embeddings)\n",
    "val_image_embeddings = np.array(val_image_embeddings)\n",
    "\n",
    "print(f\"\\n –í–µ–∫—Ç–æ—Ä–∏–∑–∞—Ü–∏—è –∏–∑–æ–±—Ä–∞–∂–µ–Ω–∏–π –∑–∞–≤–µ—Ä—à–µ–Ω–∞!\")\n",
    "print(f\"   Train embeddings: {train_image_embeddings.shape}\")\n",
    "print(f\"   Val embeddings: {val_image_embeddings.shape}\")\n",
    "print(f\"   –†–∞–∑–º–µ—Ä–Ω–æ—Å—Ç—å –≤–µ–∫—Ç–æ—Ä–∞: {train_image_embeddings.shape[1]} (ResNet50 output)\")\n",
    "\n",
    "# –û—Å–≤–æ–±–æ–∂–¥–∞–µ–º –ø–∞–º—è—Ç—å\n",
    "torch.cuda.empty_cache()"
