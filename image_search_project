    "\n",
    "    STOP_WORDS = [\n",
    "        'child', 'children', 'kid', 'kids', 'boy', 'boys', 'girl', 'girls',\n",
    "        'baby', 'babies', 'teen', 'teenager', 'teenagers', 'minors', 'underage',\n",
    "        'youth', 'young', 'juvenile', 'schoolboy', 'schoolgirl', 'toddler',\n",
    "        'infant', 'newborn', 'preschooler'\n",
    "    ]\n",
    "\n",
    "    # –ü—Ä–æ–≤–µ—Ä—è–µ–º –∑–∞–ø—Ä–æ—Å –Ω–∞ —Å—Ç–æ–ø-—Å–ª–æ–≤–∞\n",
    "    query_lower = str(query_text).lower()\n",
    "    prohibited = False\n",
    "    matched_words = []\n",
    "\n",
    "    for word in STOP_WORDS:\n",
    "        pattern = r'\\b' + re.escape(word) + r'\\b'\n",
    "        if re.search(pattern, query_lower):\n",
    "            prohibited = True\n",
    "            matched_words.append(word)\n",
    "\n",
    "    if prohibited:\n",
    "        print(f\" –Æ–†–ò–î–ò–ß–ï–°–ö–ò–ô –§–ò–õ–¨–¢–† –°–†–ê–ë–û–¢–ê–õ\")\n",
    "        print(f\" –ó–∞–ø—Ä–æ—Å —Å–æ–¥–µ—Ä–∂–∏—Ç –∑–∞–ø—Ä–µ—â—ë–Ω–Ω—ã–µ —Å–ª–æ–≤–∞: {', '.join(matched_words)}\")\n",
    "        print(f\" This image is unavailable in your country in compliance with local laws\")\n",
    "        return []\n",
    "\n",
    "    # ============================================================================\n",
    "    # –í–µ–∫—Ç–æ—Ä–∏–∑–∞—Ü–∏—è –∑–∞–ø—Ä–æ—Å–∞\n",
    "    # ============================================================================\n",
    "\n",
    "    try:\n",
    "        tokens = clip.tokenize([str(query_text)], truncate=True).to(device)\n",
    "\n",
    "        with torch.no_grad():\n",
    "            text_embedding = clip_model.encode_text(tokens)\n",
    "            text_embedding = text_embedding / text_embedding.norm(dim=-1, keepdim=True)\n",
    "\n",
    "        text_embedding = text_embedding.squeeze().cpu().numpy()\n",
    "    except Exception as e:\n",
    "        print(f\" –û—à–∏–±–∫–∞ –ø—Ä–∏ –≤–µ–∫—Ç–æ—Ä–∏–∑–∞—Ü–∏–∏ —Ç–µ–∫—Å—Ç–∞: {e}\")\n",
    "        return []\n",
    "\n",
    "    # ============================================================================\n",
    "    # –ü–æ–∏—Å–∫ –Ω–∞–∏–±–æ–ª–µ–µ –ø–æ—Ö–æ–∂–∏—Ö –∏–∑–æ–±—Ä–∞–∂–µ–Ω–∏–π\n",
    "    # ============================================================================\n",
    "\n",
    "    similarities = []\n",
    "\n",
    "    for img_name, img_embedding in test_image_embeddings.items():\n",
    "        # –ö–æ—Å–∏–Ω—É—Å–Ω–∞—è —Å—Ö–æ–∂–µ—Å—Ç—å\n",
    "        raw_similarity = np.dot(img_embedding, text_embedding)\n",
    "\n",
    "        # –ö–∞–ª–∏–±—Ä–æ–≤–∫–∞ –ø–æ–¥ —à–∫–∞–ª—É [0, 1]\n",
    "        calibrated_score = calibrator.predict([[raw_similarity]])[0]\n",
    "        calibrated_score = np.clip(calibrated_score, 0, 1)\n",
    "\n",
    "        similarities.append((img_name, raw_similarity, calibrated_score))\n",
    "\n",
    "    # –°–æ—Ä—Ç–∏—Ä—É–µ–º –ø–æ –∫–∞–ª–∏–±—Ä–æ–≤–∞–Ω–Ω–æ–π –æ—Ü–µ–Ω–∫–µ (—É–±—ã–≤–∞–Ω–∏–µ)\n",
    "    similarities.sort(key=lambda x: x[2], reverse=True)\n",
    "\n",
    "    # ============================================================================\n",
    "    # –í—ã–≤–æ–¥ —Ä–µ–∑—É–ª—å—Ç–∞—Ç–æ–≤\n",
    "    # ============================================================================\n",
    "\n",
    "    print(f\"\\n –ó–∞–ø—Ä–æ—Å: '{query_text}'\")\n",
    "    print(f\"{'=' * 70}\")\n",
    "    print(f\"{'–†–∞–Ω–≥':<6} {'–ò–∑–æ–±—Ä–∞–∂–µ–Ω–∏–µ':<30} {'–°—Ö–æ–∂–µ—Å—Ç—å':<12} {'–û—Ü–µ–Ω–∫–∞':<10}\")\n",
    "    print(f\"{'=' * 70}\")\n",
    "\n",
    "    results = similarities[:top_k]\n",
    "\n",
    "    for rank, (img_name, raw_sim, cal_score) in enumerate(results, 1):\n",
    "        print(f\"{rank:<6} {img_name:<30} {raw_sim:<12.4f} {cal_score:<10.4f}\")\n",
    "\n",
    "    # –ü–æ–∫–∞–∑—ã–≤–∞–µ–º –∏–∑–æ–±—Ä–∞–∂–µ–Ω–∏—è\n",
    "    if show_image and results:\n",
    "        fig, axes = plt.subplots(1, min(top_k, 4), figsize=(4 * min(top_k, 4), 5))\n",
    "        if top_k == 1:\n",
    "            axes = [axes]\n",
    "\n",
    "        for i, (img_name, _, _) in enumerate(results[:min(top_k, 4)]):\n",
    "            img_path = test_image_paths[img_name]\n",
    "            img = Image.open(img_path)\n",
    "            axes[i].imshow(img)\n",
    "            axes[i].set_title(f\"–†–∞–Ω–≥ {i+1}\\n{img_name[:20]}...\", fontsize=9)\n",
    "            axes[i].axis('off')\n",
    "\n",
    "        plt.tight_layout()\n",
    "        plt.show()\n",
    "\n",
    "    return results"
   ]
  },
